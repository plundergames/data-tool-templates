{
  "name": "data-tool-templates",
  "version": "1.0.0",
  "description": "Default templates for the correlation data tool",
  "main": "build.js",
  "type": "module",
  "scripts": {
    "build": "node .",
    "watch:templates": "npm-watch"
  },
  "watch": {
    "build":  {
      "patterns": [
        "templates"
      ],
      "extensions": "json",
      "quiet": false
    }
  },
  "author": "",
  "dependencies": {
    "npm-watch": "^0.11.0"
  }
}
